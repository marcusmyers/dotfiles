{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
    "black": "#262B44",
    "blue": "#4B95E9",
    "green": "#59C9A5",
    "lime-green": "#97ca3d",
    "orange": "#F07623",
    "red": "#D81E5B",
    "white": "#E0DEF4",
    "yellow": "#F3AE35"
  },
  "secondary_prompt": {
    "template": "<p:lime-green,transparent>\ue0b6</><,p:lime-green> > </><p:lime-green,transparent>\ue0b0</> ",
    "foreground": "p:black",
    "background": "transparent"
  },
  "transient_prompt": {
    "template": "<p:lime-green,transparent>\ue0b6</><,p:lime-green> {{ .Folder }} </><p:lime-green,transparent>\ue0b0</> ",
    "foreground": "p:black",
    "background": "transparent"
  },
  "console_title_template": "{{ .Shell }} in {{ .Folder }}",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "style": "diamond",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b0",
          "template": " {{ if .WSL }}WSL at {{ end }}{{.Icon}} ",
          "foreground": "#26C6DA",
          "background": "#546E7A",
          "type": "os"
        },
        {
          "properties": {
            "max_depth": 3,
            "style": "agnoster_short"
          },
          "style": "powerline",
          "template": " \uea83 {{ path .Path .Location }} ",
          "foreground": "p:white",
          "powerline_symbol": "\ue0b0",
          "background": "p:blue",
          "type": "path"
        },
        {
          "properties": {
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "style": "powerline",
          "template": " {{ if .UpstreamURL }}{{ url .UpstreamIcon .UpstreamURL }} {{ end }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }} ",
          "foreground": "p:black",
          "powerline_symbol": "\ue0b0",
          "background": "p:green",
          "type": "git",
          "foreground_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}p:black{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}p:white{{ end }}",
            "{{ if gt .Ahead 0 }}p:white{{ end }}"
          ],
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}p:yellow{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}p:red{{ end }}",
            "{{ if gt .Ahead 0 }}#49416D{{ end }}",
            "{{ if gt .Behind 0 }}#7A306C{{ end }}"
          ]
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "properties": {
            "always_enabled": true
          },
          "style": "diamond",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b0",
          "template": " {{ if gt .Code 0 }}\uf00d{{ else }}\uf00c{{ end }} ",
          "foreground": "p:white",
          "background": "p:blue",
          "type": "status",
          "background_templates": [
            "{{ if gt .Code 0 }}p:red{{ end }}"
          ]
        },
        {
          "style": "powerline",
          "template": " \ue791 {{ .Full }}",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "background": "#4063D8",
          "type": "ruby"
        },
        {
          "style": "powerline",
          "template": " \ue73d {{ .Full }} ",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "background": "#4063D8",
          "type": "php"
        },
        {
          "properties": {
            "always_enabled": true,
            "style": "austin",
            "threshold": 500
          },
          "style": "powerline",
          "template": " <#fefefe>\ueba2</> {{ .FormattedMs }} ",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "background": "#8800dd",
          "type": "executiontime"
        },
        {
          "properties": {
            "time_format": " 15:04:05"
          },
          "style": "powerline",
          "template": " {{ .CurrentDate | date .Format }} ",
          "foreground": "#007ACC",
          "powerline_symbol": "\ue0b0",
          "background": "p:white",
          "type": "time"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "style": "plain",
          "template": "\u2570\u2500",
          "foreground": "#21c7c7",
          "background": "",
          "type": "text"
        },
        {
          "properties": {
            "always_enabled": true
          },
          "style": "plain",
          "template": "‚ùØ  ",
          "foreground": "#e0f8ff",
          "background": "",
          "type": "status"
        }
      ],
      "newline": true
    }
  ],
  "version": 3,
  "final_space": true
}
